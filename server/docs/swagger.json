                    },
                    "500": {
                        "description": "Fail to generate gif",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/gif/heart-locket": {
            "post": {
                "description": "Use image-lef and image-right files from form to make a opening heart locket gif",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "json image/gif"
                ],
                "tags": [
                    "Gif"
                ],
                "summary": "Create a heart locket gif",
                "parameters": [
                    {
                        "type": "file",
                        "description": "A png or jpg image to left half",
                        "name": "image-left",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "A png or jpg image to right half",
                        "name": "image-right",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "enum": [
                            "standard",
                            "christmas"
                        ],
                        "type": "string",
                        "default": "standard",
                        "description": "Variant of heart locket gif",
                        "name": "variant",
                        "in": "formData"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Generated Gif",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "400": {
                        "description": "Fail to load image from form",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Fail to generate gif",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/gif/nesting-doll": {
            "post": {
                "description": "Use three images to create a nesting doll gif",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "json image/gif"
                ],
                "tags": [
