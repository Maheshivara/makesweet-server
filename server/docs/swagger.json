{
    "consumes": [
        "multipart/form-data"
    ],
    "produces": [
        "json image/gif"
    ],
    "swagger": "2.0",
    "info": {
        "description": "A golang server to create gifs from images.",
        "title": "Makesweet golang server",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api",
    "paths": {
        "/gif/billboard": {
            "post": {
                "description": "Use image from form to make a approaching billboard gif",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "json image/gif"
                ],
                "tags": [
                    "Gif"
                ],
                "summary": "Create a billboard gif",
                "parameters": [
                    {
                        "type": "file",
                        "description": "A png or jpg image",
                        "name": "image",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Generated Gif",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "400": {
                        "description": "Fail to load image from form",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Fail to generate gif",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/gif/circuit": {
            "post": {
                "description": "Use image from form to make a zooming circuit board gif",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "json image/gif"
                ],
                "tags": [
                    "Gif"
                ],
                "summary": "Create a circuit board gif",
                "parameters": [
                    {
                        "type": "file",
                        "description": "A png or jpg image",
                        "name": "image",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Generated Gif",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "400": {
                        "description": "Fail to load image from form",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Fail to generate gif",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/gif/custom": {
            "post": {
                "description": "Use images and a template from form to make a custom gif",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "json image/gif"
                ],
                "tags": [
                    "Gif"
                ],
                "summary": "Create a custom gif",
                "parameters": [
                    {
                        "type": "array",
                        "items": {
                            "type": "file"
                        },
                        "description": "A png or jpg image array",
                        "name": "images",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "A zip template file",
                        "name": "template",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Generated Gif",
                        "schema": {
                            "type": "file"
                        }
                    },
                    "400": {
                        "description": "Fail to load images from form",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Fail to generate gif",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/gif/flag": {
            "post": {
                "description": "Use image from form to make a waving flag gif",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "json image/gif"
